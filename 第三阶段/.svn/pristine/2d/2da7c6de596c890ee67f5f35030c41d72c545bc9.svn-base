.radio-width{
    display: inline-block;
    width: 33%;
    box-sizing: border-box;
}
.radio-style{
    position: relative;
}
.radio-style input[type=radio],.radio-style input[type=checkbox]  {
    margin: 0;
    opacity: 0;
    cursor: pointer;
    /*width: 30px;
    top: 3px;
    height: 30px;*/
    width: 0.4rem;
    height: 0.4rem;
    z-index: 10;
    position: absolute;
    top: 0.133333rem;
}
.radio-style label{
    display: inline-block;
    position: relative;
    padding-left: 1.133333rem;
    font-size: 0.426667rem;
    color: #888888;
}
.radio-style input[type=radio] + label::before,.radio-style input[type=checkbox] + label::before {
    content: "";
    position: absolute;
    /*width: 30px;
    height: 30px;*/
    width: 0.4rem;
    height: 0.4rem;
    left: 0;
    top: 0.133333rem;
    /*top: 3px;*/
    border: 1px solid #cccccc;
    border-radius: 50%;
    background-color: #fff;
    box-sizing: border-box;
}
.radio-style input[type=radio]:checked + label::before,.radio-style input[type=checkbox]:checked + label::before{
    border: none;
    background-color: #4993f7;
}

.radio-style input[type=radio] +  label::after,.radio-style input[type=checkbox] +  label::after{
    position: absolute;
    content: " ";
    /*width: 12px;
    height: 12px;
    left: 9px;
    top: 9px;*/
    width: 0.16rem;
    height: 0.16rem;
    left: 0.12rem;
    top: 0.253333rem;
    border-radius: 50%;
    background-color: #fff;
}

.radio-style input[type=radio]:checked +  label::after,.radio-style input[type=checkbox]:checked +  label::after{
    background-color: #fff;
}





