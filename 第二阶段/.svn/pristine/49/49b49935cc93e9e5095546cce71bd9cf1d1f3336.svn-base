@charset "utf-8";
/* CSS Document */
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, textarea, p, blockquote, th, td {
    margin: 0;
    padding: 0;
    font-family: '微软雅黑';
    box-sizing: border-box;
    list-style: none;
}
.header{
	background: url('../img/header-bg.png') no-repeat;
	background-size: 100% 100%;
	height: 56px;
	padding:0 0 0 10px;
}
.logo-containter{
	display:table-cell;
	vertical-align:middle;
	height: 56px;
	float: left;
	padding:9.5px 0;
}
.header .logo-containter img{
	display: inline-block;
	vertical-align: middle;
}
.type{
	background: url('../img/type.png') no-repeat;
	float: left;
	color: #057ef0;
	font-size: 12px;
	width: 46px;
	height: 20px;
	margin:18px 0 0 10px;
	padding:0 5px;
}
.header-nav{
	float: right;
}
.pageList{
	float: left;
}
.pageList li{
	float: left;
	height: 56px;
	line-height: 56px;
	width: 105px;
	font-size: 16px;
	color: #fff;
	text-align: center;
	cursor: pointer;
}
.pageList .index-page{
	width: 77px;
}
.pageList li a{
	display: inline-block;
	width: 100%;
	height: 100%;
	font-size: 16px;
	color: #fff;
	text-decoration: none;
	box-sizing: border-box;
}
.pageList li:hover,.pageList .pageActive{
	background: url('../img/li-bg.png') repeat-x;
	border-right:1px solid #182333;
	border-left:1px solid #182333;
	/*box-shadow: 0px 0px 0px 0px #ff0000,   
                -1px 0px 0px 0px #eee,   
                1px 0px 0px 0px #eee,    
                0px 0px 0px 0px #eede15;*/
}
.pageList li:hover a,.pageList .pageActive a{
	border-left:1px solid #3b8fef;
	border-right:1px solid #3b8fef;
}
.decision-width li{
	width: 95px;
}
.efficiency-width li{
	width: 104px;
}
.baseList{
	float: right;
	border-left: 1px solid #000204;
}
.baseList li{
	float: left;
	height: 56px;
	line-height: 20px;
	text-align: center;
	cursor: pointer;
	border-right: 1px solid #000204;
	border-left: 1px solid #1d3e72;
}
.baseList .borderLeftNone{
	border-left: none;
}
.baseList li a{
	text-decoration: none;
	display: inline-block;
	width: 100%;
	height: 100%;
	box-sizing: border-box;
}
.baseList li:hover{
	background: url('../img/li-bg.png') repeat-x;
}
.baseList li:hover a{
	border-left:1px solid #3b8fef;
	border-right:1px solid #3b8fef;
}
.baseList li a span{
	font-size: 12px;
	color: #fff;
}
.news-nav{
	width: 66px;
}
.user-nav{
	width: 77px;
}
.exit-nav{
	width: 63px;
}
.baseList li div{
	width: 25px;
	height: 25px;
	margin:6.5px auto 0;
}
.remind{
	background: url('../img/remind.png') no-repeat center;
}
.user{
	background: url('../img/user.png') no-repeat center;	
}
.exit{
	background: url('../img/exit.png') no-repeat center;	
}
.news-alert{
	display: block;
	width: 15px;
	height: 15px;
	line-height: 15px;
	background-color: #ee4646;
	border:1px solid #c61717;
	color: #fff;
	border-radius: 10px;
	font-size: 12px;
	margin-left: 14px;
}
