@charset "utf-8";
/* CSS Document */
@font-face {
	font-family:GOTHICB;
	src: url('../fonts/GOTHICB.TTF');
}
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, textarea, p, blockquote, th, td {
    margin: 0;
    padding: 0;
    font-family: '微软雅黑';
    box-sizing: border-box;
}
.contain{
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	position: absolute;
	background: url('../img/bgOne.png') no-repeat;
	background-size: cover;
	background-attachment:fixed;
	min-width: 1366px;
	min-height: 768px;
}
.energy-bg{
	background-image: url('../img/energy-bg.png');
	background-repeat: no-repeat;
	background-size: cover;
	background-attachment:fixed;
}
.main{
	top: 56px;
	right: 0;
	bottom: 0;
	left: 0;
	position: absolute;
	padding:9px 10px;
}
.index-left{
	float: left;
	width: 25%;
	height:100%;
	border:1px solid #0c56a7;
	border-radius:5px;
}
.index-right{
	overflow: hidden;
	float: none;
	height: 100%;
	margin-left: 25%;
	padding-left: 10px;
	position: relative;
}
.index-right-top{
	border:1px solid #0c56a7;
	width: 100%;
	height: 50%;
	position: relative;
}
.index-right-bottom{	
	position: absolute;
    top: 50%;
    bottom: 0px;
    left: 10px;
    right: 0;
}
.bottom-left{
	position: absolute;
	top: 10px;
	bottom: 0;
	width: 33.7%;
	border:1px solid #0c56a7;
}
.bottom-right{
	margin-left: 10px;
	position: absolute;
	top: 10px;
	bottom: 0;
	left: 33.7%;
	right: 0;
	border:1px solid #0c56a7;
}
.left-one{
	height: 28.2%;
	position: relative;
}
.left-two{
	height: 33.7%;
	position: relative;
}
.left-three{
	height: 38.1%;
	position: relative;
}
.left-content h2{
	height: 43px;
	border-bottom: 1px solid #0c54a3;
	font-size: 18px;
	color: #ffffff;
	line-height: 43px;
	background: url('../img/h2-bg.png') repeat-x;
	padding:0 15px 0 13px;
	box-shadow: 0 5px 29px -10px #000000;
}
.left-content h2 input{
	float: right;
	height: 28px;
	width: 54px;
	border: 1px solid #0e53a2;
	font-size: 14px;
	color: #ffffff;
	line-height: 28px;
	background: url('../img/input-bg.png') repeat-x;
	border-radius: 3px;
	margin-top: 6.5px;
}
.news-div{
	border-top: 1px solid #010712;
	position: absolute;
	top: 43px;
	bottom: 0;
	left: 0;
	right: 0;
}
.news-ul{
	position: absolute;
	top: 5px;
	bottom: 5px;
	left: 0;
	right: 2px;	
}
.news-ul-one,.news-ul-two{
	overflow: hidden;
}
.scrollbar{
	height: 300px;
}
.news-ul ul div{
	height: 18px;
	line-height: 18px;
	width: 19px;
	float: left;
	background: #082e6b;
	border-radius: 3px;
	text-align: center;
	color: #fff;
	font-size: 12px;
	margin-top:8.5px; 
}
.news-ul li{
	height: 38px;
	margin:0 15px;
	line-height: 38px;
}
.item-not-last{
	border-bottom: 1px dashed #073e83;
}
.item-last{
	border-bottom: none;
}
.news-ul a{
	color: #fff;
	font-size: 14px;
	float: left;
	text-decoration: none;
	margin-left: 15px;
}
.news-ul span{
	color: #96c7ff;
	font-size: 13px;
	float: right;
}
.left-alert{
	display: inline-block;
	width:24px;
	height: 16px;
	line-height: 16px;
	text-align: center;
	background: #ee4646;
	color: #ffffff;
	font-size: 11px;
	border-radius: 10px;
	margin-left: 10px;
}
.wait-ul{
	position: absolute;
	top: 5px;
	bottom: 5px;
	left: 0;
	right: 2px;
	/*overflow: auto;*/
	overflow: hidden;
}
.wait-ul li{
	height: 73px;
	margin:0 15px;
	position: relative;
}
.wait-ul li img{
	margin:16px 15px 16px 0;
}
.wait-ul li a{
	color: #ffffff;
	font-size: 14px;
	line-height: 26px;
	text-align: justify;
	text-decoration: none;
	position: absolute;
	top: 11px;
	left: 49px;
}
.wait-ul li span{
	color: #b1cefc;
	font-size: 12px;
	position: absolute;
	bottom: 12px;
	right: 0;
}

.scrollbar-style::-webkit-scrollbar {/*滚动条整体样式*/
    width: 10px;     /*高宽分别对应横竖滚动条的尺寸*/
    height: 1px;
}
.scrollbar-style::-webkit-scrollbar-thumb {/*滚动条里面小方块*/
    border-radius: 10px;
     -webkit-box-shadow: inset 0 0 5px rgba(0,0,0,0.2);
    background: #535353;
}
.scrollbar-style::-webkit-scrollbar-track {/*滚动条里面轨道*/
    -webkit-box-shadow: inset 0 0 5px rgba(0,0,0,0.2);
    border-radius: 10px;
    background: #EDEDED;
}
.h2-title,.pop-title{
	height: 43px;
	border-bottom: 1px solid #0c54a3;
	font-size: 18px;
	color: #ffffff;
	line-height: 43px;
	background: url('../img/h2-bg.png') repeat-x;
	padding:0 13px;
	box-shadow: 0 5px 29px -10px #000000;
}
.bottom-left h2{
	padding:0 15px;
}
.h2-title div{
	height: 43px;
	float: right;
	font-size: 12px;
	color: #b1cefc;
}
.h2-title img{
	margin-right: 8px;
	vertical-align: middle;
}
.marginRight2{
	margin-right: 2px;
}
.h2-title span{
	position: relative;
	top: 1px;
}
.top-content{
	padding: 17px 0 16px 15px;
	position: absolute;
	top:43px;
	left: 0;
	right: 0;
	bottom: 0;
	border-top:1px solid #000206;
}
.each-data{
	height: 48%;
	width: 32.25%;
	margin:0 15px 15px 0;
	border:1px solid #010308; 
	float: left;
}
.data-thisYear{
	height: 72.4%;
	border:1px solid #09367f;
	border-bottom: 1px solid #010408;
	background: #072b66;
	position: relative;	
}
.data-thisYear img{
	position: absolute;
	top: 50%;
	margin:-22px 28px 0 27px;
}
.data-content{
	position: absolute;
	left:97px;
	right: 0;
	bottom: 0;
	top: 0;
}
.data-content h3{
	font-size: 15px;
	color: #ffffff;
	position: absolute;
	top: 20%;
	left: 0;
}
.data-content p{	
	position: absolute;
	top: 53%;
	left: 0;
}
.number{
	font-size: 26px;
	margin-right: 10px;
	font-family: 'GOTHICB';
	background-image: -webkit-linear-gradient(top, #34a0fc,#00ebff);
    -webkit-background-clip: text; /*必需加前缀 -webkit- 才支持这个text值 */
    -webkit-text-fill-color: transparent; /*text-fill-color会覆盖color所定义的字体颜色： */
}
.unit{
	font-size: 12px;
	color: #b1cefc;
}
.data-comparison{
	background: #002460;
	border-top:1px solid #17386e;
	border-bottom: 1px solid #093680;
	height: 27.6%;
	padding:0 10px;
	position: relative;
}
.comparison-left{
	position: absolute;
	top:50%;
	left:10px;
	margin-top: -8px;
	color: #b1cefc;
	font-size:12px;
}
.comparison-right{
	position: absolute;
	top:50%;
	right:10px;
	margin-top: -8px;
	float: right;
	font-size:12px;
	vertical-align:middle;
	display:table-cell;
}
.comparison-right img{
	margin-right: 8px;
}
.add-color{
	color: #ee4646;	
}
.decline-color{
	color: #2bd781;
}
.left-charts{
	position: absolute;
	top: 43px;
	bottom: 0;
	right: 0;
	left: 0;
	border-top:1px solid #000000;
}
#bing,#bar{
	height: 87%;
	border-bottom:1px solid #011d4d; 
}
.bing-legend{
	height: 13%;
	border-top: 1px solid #000;
	position: relative;
}
.legend-content{
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -173px;
	margin-top: -10.5px;
}
.legend-content2{
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -145.5px;
	margin-top: -10.5px;
}
.bing-legend .legend-content div,.bing-legend .legend-content2 div,.legend-content3 div{
	display: inline-block;
}
.bing-legend div img,.legend-content3 div img{
	margin-right: 7px;
}
.bing-legend div span,.legend-content3 div span{
	color: #b1cefc;
	font-size: 12px;
}
.topborder{
	border-top: 1px solid #193361
}
.right22{
	margin-right: 22px;
}
.right0{
	margin-right: 0;
}
.auto{
	overflow: auto;
}
.hidden{
	overflow: hidden;
}



/*决策支持页面*/
.decision-main{
	top: 56px;
	right: 0;
	bottom: 0;
	left: 0;
	position: absolute;
	padding:10px 0 10px 10px;
}
.decision-top{
	height:30.5%;
	display: flex;
	justify-content:space-between;
}
.each-scene{
	height: 100%;
	border:1px solid #000;
	border-top:none;
	flex: 1;
	margin-right:10px;
	border-radius: 3px; 
	background: rgba(7,43,102,0.5);
}
.each-scene div{
	height: 100%;
	width: 100%;
	border:1px solid #09357d;
	border-top:none;
	border-radius: 3px; 
}
.each-scene h3{
	height: 44px;
	font-size: 16px;
	color: #ffffff;
	line-height: 44px;
	background: url('../img/h2-bg.png') repeat-x;
	text-align: center;
	border:1px solid #0c56a7;
	border-bottom: none;
	border-radius: 3px 3px 0 0;
	box-shadow: 0 5px 29px -10px #000000;
	margin:0 -1px;
}
.emission{
	font-size: 16px;
	color: #96c7ff;
	text-align: center;
	margin-top: 7.5%;
}
.emission-data{
	font-size: 44px;
	text-align: center;
	font-family: GOTHICB;
	margin-top: 3.5%;
}
.data-color1{
	background-image: -webkit-linear-gradient(top, #34a0fc,#00ebff);
    -webkit-background-clip: text; /*必需加前缀 -webkit- 才支持这个text值 */
    -webkit-text-fill-color: transparent; /*text-fill-color会覆盖color所定义的字体颜色： */
}
.data-color2{
	background-image: -webkit-linear-gradient(top, #12f550,#63ff9e);
    -webkit-background-clip: text; /*必需加前缀 -webkit- 才支持这个text值 */
    -webkit-text-fill-color: transparent; /*text-fill-color会覆盖color所定义的字体颜色： */
}
.data-color3{
	background-image: -webkit-linear-gradient(top, #ff8400,#ffba00);
    -webkit-background-clip: text; /*必需加前缀 -webkit- 才支持这个text值 */
    -webkit-text-fill-color: transparent; /*text-fill-color会覆盖color所定义的字体颜色： */
}
.data-color4{
	background-image: -webkit-linear-gradient(top, #d300ff,#ff00fc);
    -webkit-background-clip: text; /*必需加前缀 -webkit- 才支持这个text值 */
    -webkit-text-fill-color: transparent; /*text-fill-color会覆盖color所定义的字体颜色： */
}
.emission-unit{
	font-size: 12px;
	color: #b1cefc;
	text-align: center;
	margin-top: 3.5%;
}
.decision-bottom{
	border:1px solid #0c56a7;
	position: absolute;
	top:30.9%;
	bottom: 10px;
	left: 10px;
	right: 10px;
	margin-top: 10px;
	border-radius: 2px;
}
.decision-bottom h2{
	height: 43px;
	border-top: none;
	font-size: 16px;
	color: #ffffff;
	line-height: 44px;
	background: url('../img/h2-bg.png') repeat-x;
	padding-left: 15px;
	box-shadow: 0 5px 29px -10px #000000;
}
.decision-charts{
	position: absolute;
	top:43px;
	left: 0;
	right: 0;
	bottom: 0;
	background: rgba(0,41,103,0.5);
}
#line{
	height:91%;
	border-top:1px solid #03204e; 
}
.bar-line-legend{
	height:9%;
	margin:0 25px;
	border-top:1px solid #000000;
	position: relative; 
}
.decision-border{
	border-top:1px solid #193a6c;
}
.legend-content3{
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -209.5px;
	margin-top: -10.5px;
}

/*能效管理页面*/
.energy-main{
	top: 56px;
	right: 0;
	bottom: 0;
	left: 0;
	position: absolute;
	padding:13px 13px 15px;
}
.left-tab{
	width: 82px;
	float: left;
}
.each-tab{
	height: 82px;
	border:1px solid #000000;	
	/*box-shadow: 0 5px 11px -3px #040000;*/
	box-shadow: 0 5px 29px -10px #000000;
	position: relative;
	margin-bottom: 8px;
	border-radius: 3px;
}
.each-tab-border{
	border:1px solid #122d56;
	height: 100%;
	background:#002967;
	border-radius: 3px;
	cursor: pointer;
}
.each-tab h3{
	color: #ffffff;
	font-size: 13px;
	text-align: center;
}
.energy-icon1{
	margin:14px 27px 10px;
}
.energy-icon2{
	margin:14px 26px 10px;
}
.energy-icon3{
	margin:14px 25.5px 10px;
}
.energy-icon4{
	margin:14px 26px 10px;
}
.tringle-right{
	position: absolute;
	right: -6px;
	top:50%;
	margin-top: -6px;
	display: none;
}
.each-tab:hover .each-tab-border,.tabAcive{
	border:1px solid #179cef;
	background: url('../img/energy-icon-bg.png') repeat-x;
}
.each-tab:hover .tringle-right{
	display: block;
}
.energy-right{
	float: right;
	width: 42.6%;
	height: 100%;
	position: relative;
	/*border:1px solid #fff;*/
}
.energy-right-top{
	height: 48.5%;
	border:1px solid #0c56a7;
	border-radius: 2px;
	position: relative;
}
.energy-right-bottom{
	margin-top: 19px;
	position: absolute;
    top: 48.5%;
    bottom: 0px;
    left: 0;
    right: 0;
}
.energy-right-bottom1{
	position: absolute;
	top:0;
	border:1px solid #0c56a7;
	border-radius: 2px;
	width: 45%;
	bottom: 0;
}
.energy-right-bottom2{
	position: absolute;
	top:0;
	left: 45%;
	right: 0;
	border:1px solid #0c56a7;
	border-radius: 2px;
	margin-left: 13px;
	bottom: 0;
}
.project-content{
	position: absolute;
	top:43px;
	bottom: 0;
	left: 0;
	right: 0;
	padding:13px 17px 4px 13px;
	border-top:1px solid #000;
	background: rgba(2,38,93,0.8);
}
#pie,#bar{
	width: 100%;
	height: 100%;
}
.indicator-padding{
	padding:0  14px 10px 0;
