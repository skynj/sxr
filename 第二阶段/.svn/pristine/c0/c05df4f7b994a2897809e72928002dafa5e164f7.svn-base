}
.infor-left{
	color: #b1cefc;
	font-size: 16px;
	margin-top: -10.5px;
	position: absolute;
	top:50%;
	left: 23px;
}
.infor-right{	
	margin-top: -19.5px;
	position: absolute;
	top:50%;
	right: 23px;
}
.infor-data{
	color: #2bd781;
	font-size: 32px;
	font-family: 'GOTHICB';
